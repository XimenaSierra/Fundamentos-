
	Proceso operadores_unitarios
	Escribir "Digite el primer numero: ";
	Leer n1;
	Escribir "Digite el segundo numero: ";
	Leer n2;
	suma<-n1+n2;
	Escribir "El resultado de la suma de los dos digitos introducidos es...", suma;
	resta<-n1-n2;
	Escribir "El resultado de la suma de los dos digitos introducidos es...", resta;

FinProceso
